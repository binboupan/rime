Services:
    github:
        plugin: website
        name: "Github"
        subtitle: "Developer Platform"
        url: "https://github.com"
        ping_url: "https://github.com"
        interval: 60
        icon: "github"
    pihole:
        plugin: pihole
        name: "Pi-hole"
        api-key: ""
        subtitle: "Ad Blocker"
        url: "https://pi.hole"
        interval: 60
        icon: "pihole"
    adguardhome:
        plugin: adguardhome
        name: "Adguard Home"
        subtitle: "Ad Blocker"
        # api key is based64 of username:password
        api-key: "base64 string"
        url: "https://adguard"
        interval: 60
        icon: "adguard"

